package Utility;

import org.openqa.selenium.chrome.ChromeDriver;

public class Temp
{

	public static void main(String[]args)
	{
		System.setProperty("webdriver.chrome.driver", "D:\\AutomationSupport\\chromedriver.exe");
		
		ChromeDriver driver=new ChromeDriver();
		driver.manage().window().maximize();
		
		driver.get("https://www.flipkart.com/");
		driver.findElementByXPath("//*[@type='password']").sendKeys("jadhav.dhanshree01@gmail.com");
		
		
	}
	
	
	
	
}
